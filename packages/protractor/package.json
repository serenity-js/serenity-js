    "@serenity-js/jasmine": "workspace:*",
    "@serenity-js/local-server": "workspace:*",
    "@serenity-js/mocha": "workspace:*",
    "@serenity-js/web": "workspace:*",
    "@types/chai": "4.3.20",
    "@types/express": "5.0.1",
    "@types/html-minifier": "4.0.5",
    "@types/mocha": "10.0.10",
    "axios": "1.9.0",
