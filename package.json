{
  "name": "serenity-js-monorepo",
  "version": "3.0.0-monorepo",
  "description": "Serenity/JS: An innovative test automation framework for creating high-quality, business-focused test scenarios that interact with any system interface and produce comprehensive reports",
  "private": true,
  "author": {
    "name": "Jan Molak",
    "email": "jan.molak@smartcodeltd.co.uk",
    "url": "https://janmolak.com"
  },
  "homepage": "https://serenity-js.org",
  "license": "Apache-2.0",
  "keywords": [
    "protractor",
    "mocha",
    "cucumber",
    "angular",
    "tdd",
    "bdd",
    "test",
    "selenium",
    "webdriverjs",
    "webdriver",
    "testing"
  ],
  "scripts": {
    "postinstall": "pnpm postinstall:protractor && pnpm postinstall:playwright && pnpm postinstall:webdriverio",
    "postinstall:playwright": "npx playwright install --with-deps",
    "postinstall:protractor": "npx tsx bin/install-browsers.ts chrome@129 chromedriver@129",
    "postinstall:webdriverio": "npx tsx bin/install-browsers.ts chrome@stable chromedriver@stable",
    "clean": "rimraf target && lerna run clean",
    "cc": "nx reset",
    "lint": "eslint --ext ts --config .eslintrc.yml .",
    "lint:fix": "eslint --ext ts --config .eslintrc.yml --fix .",
    "compile:all": "lerna run compile",
    "compile:libs": "lerna run compile --scope @serenity-js/* --scope @integration/testing-tools --scope @integration/cucumber-specs",
    "compile:@serenity-js": "lerna run compile --scope @serenity-js/*",
    "compile:@integration": "lerna run compile --scope @integration/*",
    "compile:@examples": "lerna run compile --scope @examples/*",
    "test": "lerna run test --scope @serenity-js/*",
    "test:no-coverage": "lerna run test:no-coverage --scope @serenity-js/*",
    "integration-test:all": "lerna run test --concurrency 1 --scope @integration/*",
    "integration-test:cucumber-all": "lerna run test --concurrency 1 --scope @integration/cucumber-*",
    "integration-test:cucumber-1": "lerna run test --scope @integration/cucumber-1",
    "integration-test:cucumber-2": "lerna run test --scope @integration/cucumber-2",
    "integration-test:cucumber-3": "lerna run test --scope @integration/cucumber-3",
    "integration-test:cucumber-4": "lerna run test --scope @integration/cucumber-4",
    "integration-test:cucumber-5": "lerna run test --scope @integration/cucumber-5",
    "integration-test:cucumber-6": "lerna run test --scope @integration/cucumber-6",
    "integration-test:cucumber-7": "lerna run test --scope @integration/cucumber-7",
    "integration-test:cucumber-8": "lerna run test --concurrency 1 --scope @integration/cucumber-8-*",
    "integration-test:cucumber-9": "lerna run test --concurrency 1 --scope @integration/cucumber-9",
    "integration-test:cucumber-10": "lerna run test --concurrency 1 --scope @integration/cucumber-10",
    "integration-test:cucumber-11": "lerna run test --concurrency 1 --scope @integration/cucumber-11",
    "integration-test:cucumber-12": "lerna run test --concurrency 1 --scope @integration/cucumber-12",
    "integration-test:jasmine": "lerna run test --concurrency 1 --scope @integration/jasmine",
    "integration-test:mocha": "lerna run test --concurrency 1 --scope @integration/mocha",
    "integration-test:playwright-all": "lerna run test --concurrency 1 --scope @integration/playwright-*",
    "integration-test:playwright-test": "lerna run test --scope @integration/playwright-test",
    "integration-test:playwright-test-ct": "lerna run test --scope @integration/playwright-test-ct",
    "integration-test:rest": "lerna run test --scope @integration/rest",
    "integration-test:playwright-web": "lerna run test --scope @integration/playwright-web",
    "integration-test:protractor-all": "lerna run test --concurrency 1 --scope @integration/protractor-*",
    "integration-test:protractor-test-runners": "lerna run test --concurrency 1 --scope @integration/protractor-cucumber --scope @integration/protractor-jasmine --scope @integration/protractor-mocha",
    "integration-test:protractor-cucumber": "lerna run test --scope @integration/protractor-cucumber",
    "integration-test:protractor-jasmine": "lerna run test --scope @integration/protractor-jasmine",
    "integration-test:protractor-mocha": "lerna run test --scope @integration/protractor-mocha",
    "integration-test:protractor-web": "lerna run test --scope @integration/protractor-web",
    "integration-test:webdriverio-8-all": "lerna run test --concurrency 1 --scope @integration/webdriverio-8-*",
    "integration-test:webdriverio-8-test-runners": "lerna run test --concurrency 1 --scope @integration/webdriverio-8-cucumber --scope @integration/webdriverio-8-jasmine --scope @integration/webdriverio-8-mocha",
    "integration-test:webdriverio-8-cucumber": "lerna run test --scope @integration/webdriverio-8-cucumber",
    "integration-test:webdriverio-8-jasmine": "lerna run test --scope @integration/webdriverio-8-jasmine",
    "integration-test:webdriverio-8-mocha": "lerna run test --scope @integration/webdriverio-8-mocha",
    "integration-test:webdriverio-8-web": "lerna run test --scope @integration/webdriverio-8-web",
    "integration-test:webdriverio-8-web-devtools": "lerna run test:devtools --stream --scope @integration/webdriverio-8-web",
    "integration-test:webdriverio-8-web-webdriver": "lerna run test:webdriver --stream --scope @integration/webdriverio-8-web",
    "integration-test:webdriverio-web": "lerna run test --scope @integration/webdriverio-web",
    "integration-test:webdriverio-test-runners": "lerna run test --concurrency 1 --scope @integration/webdriverio-cucumber --scope @integration/webdriverio-jasmine --scope @integration/webdriverio-mocha",
    "commit": "npx tsx bin/commit.ts",
    "lerna": "lerna",
    "lerna:publish": "lerna publish --yes",
    "lerna:test:version": "lerna version --no-git-tag-version --no-push",
    "lerna:test:publish": "lerna publish --no-git-tag-version --no-push",
    "version:print": "npx tsx bin/print-versions.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@puppeteer/browsers": "2.11.2",
    "@types/chai": "4.3.20",
    "@types/chai-as-promised": "7.1.8",
    "@types/node": "24.10.9",
    "@types/sinon": "17.0.4",
    "@types/sinon-chai": "3.2.12",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "c8": "10.1.3",
    "chai": "4.5.0",
    "chai-as-promised": "7.1.2",
    "cross-env": "10.1.0",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "7.5.1",
    "eslint": "8.57.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-mocha": "10.5.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "51.0.1",
    "eslint-plugin-unused-imports": "3.2.0",
    "fast-glob": "3.3.3",
    "husky": "9.1.7",
    "inquirer": "6.5.2",
    "lerna": "9.0.3",
    "mocha": "11.7.5",
    "mocha-multi": "1.1.7",
    "mocha-testdata": "1.2.0",
    "playwright": "1.58.0",
    "rimraf": "6.1.2",
    "semver": "7.7.3",
    "sinon": "18.0.1",
    "sinon-chai": "3.7.0",
    "test-console": "2.0.0",
    "ts-node": "10.9.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/serenity-js/serenity-js.git"
  },
  "bugs": {
    "url": "https://github.com/serenity-js/serenity-js/issues"
  },
  "engines": {
    "node": "^20 || ^22 || ^24"
  },
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402"
}
