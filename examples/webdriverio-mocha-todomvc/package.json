{
  "name": "@examples/webdriverio-mocha-todomvc",
  "version": "3.0.0",
  "description": "Example implementation of a test suite using WebdriverIO and Mocha to exercise a Web interface",
  "author": {
    "name": "Jan Molak",
    "email": "jan.molak@smartcodeltd.co.uk",
    "url": "https://janmolak.com"
  },
  "homepage": "https://serenity-js.org",
  "license": "Apache-2.0",
  "private": true,
  "config": {
    "access": "private"
  },
  "scripts": {
    "clean": "rimraf target",
    "test:update-serenity": "serenity-bdd update --ignoreSSL",
    "test:acceptance": "wdio wdio.conf.ts",
    "test:report": "serenity-bdd run",
    "test": "failsafe clean test:update-serenity test:acceptance test:report",
    "verify": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/serenity-js/serenity-js.git"
  },
  "bugs": {
    "url": "https://github.com/serenity-js/serenity-js/issues"
  },
  "engines": {
    "node": "^20 || ^22 || ^24"
  },
  "devDependencies": {
    "@serenity-js/assertions": "workspace:*",
    "@serenity-js/console-reporter": "workspace:*",
    "@serenity-js/core": "workspace:*",
    "@serenity-js/mocha": "workspace:*",
    "@serenity-js/webdriverio": "workspace:*",
    "@serenity-js/serenity-bdd": "workspace:*",
    "@serenity-js/web": "workspace:*",
    "@wdio/cli": "9.21.1",
    "@wdio/globals": "9.17.0",
    "@wdio/local-runner": "9.21.0",
    "@wdio/spec-reporter": "9.20.0",
    "@wdio/reporter": "9.20.0",
    "@types/mocha": "10.0.10",
    "mocha": "11.7.5",
    "npm-failsafe": "1.4.0",
    "webdriverio": "9.21.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  }
}
