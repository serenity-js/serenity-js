{
  "name": "serenity-protractor-cucumber-pageobjects",
  "version": "1.0.0",
  "description": "Serenity.js with classic Protractor and Cucumber setup",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf target",
    "pretest": "serenity update",
    "pree2e": "npm run webdriver:update -- --standalone --versions.standalone=2.53.1 --versions.chrome=2.28",
    "e2e": "protractor ./protractor.conf.js",
    "e2e-single": "protractor ./protractor.conf.js --cucumberOpts.name",
    "report": "serenity run",
    "test": "failsafe e2e report",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update"
  },
  "author": "Jan Molak <jan.molak@smartcodeltd.co.uk>",
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "chai-smoothie": "^0.2.0",
    "cucumber": "1.2.2",
    "npm-failsafe": "^0.1.0",
    "protractor": "5.1.1",
    "protractor-cucumber-framework": "0.6.0",
    "rimraf": "2.5.4",
    "serenity-cli": "^0.1.4",
    "serenity-js": "file:../../"
  }
}
