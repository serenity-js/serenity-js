{
  "name": "@examples/webdriverio-cucumber",
  "version": "3.0.0",
  "description": "Example implementation of a test suite using WebdriverIO and Cucumber",
  "author": {
    "name": "Jan Molak",
    "email": "jan.molak@smartcodeltd.co.uk",
    "url": "https://janmolak.com"
  },
  "homepage": "https://serenity-js.org",
  "license": "Apache-2.0",
  "private": true,
  "config": {
    "access": "private"
  },
  "scripts": {
    "clean": "rimraf target",
    "test:update-serenity": "serenity-bdd update --ignoreSSL",
    "test:acceptance": "wdio wdio.conf.ts",
    "test:report": "serenity-bdd run",
    "test": "failsafe clean test:update-serenity test:acceptance test:report",
    "verify": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/serenity-js/serenity-js.git"
  },
  "bugs": {
    "url": "https://github.com/serenity-js/serenity-js/issues"
  },
  "engines": {
    "node": "^18.12 || ^20 || ^22"
  },
  "devDependencies": {
    "@cucumber/cucumber": "11.2.0",
    "@serenity-js/console-reporter": "workspace:*",
    "@serenity-js/core": "workspace:*",
    "@serenity-js/cucumber": "workspace:*",
    "@serenity-js/webdriverio": "workspace:*",
    "@serenity-js/serenity-bdd": "workspace:*",
    "@wdio/cli": "9.12.0",
    "@wdio/globals": "9.12.0",
    "@wdio/local-runner": "9.12.0",
    "@wdio/spec-reporter": "9.11.0",
    "@wdio/types": "9.10.1",
    "@wdio/reporter": "9.11.0",
    "npm-failsafe": "1.3.0",
    "webdriverio": "9.12.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  }
}
