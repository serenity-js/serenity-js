{
  "name": "@integration/webdriverio-web",
  "version": "3.0.0",
  "description": "Internal module to help test integration of Serenity/JS with WebdriverIO",
  "author": {
    "name": "Jan Molak",
    "email": "jan.molak@smartcodeltd.co.uk",
    "url": "https://janmolak.com"
  },
  "homepage": "https://serenity-js.org",
  "license": "Apache-2.0",
  "private": true,
  "config": {
    "access": "private"
  },
  "keywords": [
    "serenity-js",
    "internal"
  ],
  "scripts": {
    "clean": "rimraf target ../../target/coverage/integration-webdriverio-jasmine",
    "test:devtools": "cross-env PROTOCOL=devtools npm test",
    "test:webdriver": "cross-env PROTOCOL=webdriver npm test",
    "pretest": "serenity-bdd update --ignoreSSL",
    "test": "cross-env PORT=8080 failsafe clean test:acceptance test:report",
    "test:acceptance": "start-server-and-test start 'http://localhost:8080' test:acceptance:run",
    "start": "npx web-specs-start",
    "test:acceptance:run": "cross-env-shell nyc --report-dir='../target/coverage/integration-webdriverio-web-$PROTOCOL' wdio wdio.conf.ts",
    "test:report": "serenity-bdd run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/serenity-js/serenity-js.git"
  },
  "bugs": {
    "url": "https://github.com/serenity-js/serenity-js/issues"
  },
  "engines": {
    "node": "^14 || ^16 || ^18",
    "npm": "^6 || ^7 || ^8"
  },
  "devDependencies": {
    "@integration/testing-tools": "3.0.0",
    "@integration/web-specs": "3.0.0",
    "@serenity-js/console-reporter": "^3.0.0-rc",
    "@serenity-js/core": "^3.0.0-rc",
    "@serenity-js/mocha": "^3.0.0-rc",
    "@serenity-js/serenity-bdd": "^3.0.0-rc",
    "@serenity-js/webdriverio": "^3.0.0-rc",
    "@serenity-js/web": "^3.0.0-rc",
    "@types/mocha": "^10.0.1",
    "@wdio/cli": "^7.28.0",
    "@wdio/local-runner": "^7.28.0",
    "@wdio/spec-reporter": "^7.28.0",
    "chromedriver": "^108.0.0",
    "ci-info": "^3.7.1",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "npm-failsafe": "^0.4.3",
    "wdio-chromedriver-service": "7.3.2",
    "webdriverio": "^7.28.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
